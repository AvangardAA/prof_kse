Triangle: type = {
    a: int = ();
    b: int = ();
    c: int = ();

    operator=: (out this, a_: int, b_: int, c_: int) = {
        this.a = a_;
        this.b = b_;
        this.c = c_;
    }

    get_area: (this) -> double = {
        if (!(a + b > c && a + c > b && b + c > a)) {return -1;}

        s: double = (a + b + c) / 2.0;
        return std::sqrt(s * (s - a) * (s - b) * (s - c));
    }
}